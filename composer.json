{
    "name": "rapidez/product-alert",
    "description": "Rapidez Product Alert",
    "keywords": [
        "rapidez"
    ],
    "homepage": "https://rapidez.io",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0|^8.1",
        "rapidez/core": ">=0.90.2 <1.0.0|^1.0",
        "rapidez/account": ">=0.13.3"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\ProductAlert\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\ProductAlert\\ProductAlertServiceProvider"
            ]
        }
    }
}
